<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="match_parent"

    android:layout_height="match_parent" >


    <Button

        android:id="@+id/key3_id"

        android:layout_width="70dp"

        android:layout_height="60dp"

        android:text="3"

        android:onClick="key3Clicked"

        android:layout_below="@+id/result_id"
        android:layout_toLeftOf="@+id/Buttonclear_id"
        android:layout_toStartOf="@+id/Buttonclear_id" />

    <Button

        android:id="@+id/key9_id"

        android:layout_width="70dp"

        android:layout_height="60dp"

        android:text="9"

        android:onClick="key9Clicked"

        android:layout_below="@+id/key6_id"
        android:layout_toRightOf="@+id/key5_id"
        android:layout_toEndOf="@+id/key5_id" />

    <Button

        android:id="@+id/Buttonplus_id"

        android:layout_height="60dp"

        android:text="+"

        android:onClick="btnplusClicked"

        android:layout_width="60dp"
        android:layout_below="@+id/Buttonclear_id"
        android:layout_alignRight="@+id/Btnequal_id"
        android:layout_alignEnd="@+id/Btnequal_id" />

    <Button


        android:id="@+id/Buttonmulti_id"


        android:text="*"

        android:onClick="btnmultiClicked"

        android:layout_below="@+id/key7_id"
        android:layout_alignLeft="@+id/key7_id"
        android:layout_alignStart="@+id/key7_id"
        android:layout_height="60dp"
        android:layout_width="70dp" />

    <Button

        android:id="@+id/Btndivide_id"

        android:layout_height="60dp"

        android:text="/"

        android:onClick="btndivideClicked"


        android:layout_width="70dp"
        android:layout_below="@+id/key8_id"
        android:layout_toRightOf="@+id/Buttonmulti_id"
        android:layout_toEndOf="@+id/Buttonmulti_id" />

    <Button

        android:id="@+id/key4_id"

        android:layout_width="70dp"

        android:layout_height="60dp"

        android:text="4"

        android:onClick="key4Clicked"

        android:layout_below="@+id/Btn1_id"
        android:layout_alignLeft="@+id/Btn1_id"
        android:layout_alignStart="@+id/Btn1_id" />

    <Button

        android:id="@+id/key2_id"

        android:layout_width="70dp"

        android:layout_height="60dp"

        android:text="2"

        android:onClick="key2Clicked"

        android:layout_below="@+id/result_id"
        android:layout_toLeftOf="@+id/key3_id"
        android:layout_toStartOf="@+id/key3_id" />

    <Button

        android:id="@+id/key8_id"

        android:layout_width="70dp"

        android:layout_height="60dp"

        android:text="8"

        android:onClick="key8Clicked"

        android:layout_below="@+id/key4_id"
        android:layout_toRightOf="@+id/key7_id"
        android:layout_toEndOf="@+id/key7_id" />

    <Button

        android:id="@+id/key5_id"

        android:layout_width="70dp"

        android:layout_height="60dp"

        android:text="5"

        android:onClick="key5Clicked"

        android:layout_below="@+id/key3_id"
        android:layout_toRightOf="@+id/key4_id"
        android:layout_toEndOf="@+id/key4_id" />

    <Button

        android:id="@+id/key7_id"

        android:layout_width="70dp"

        android:layout_height="60dp"

        android:text="7"

        android:onClick="key7Clicked"

        android:layout_below="@+id/key4_id"
        android:layout_alignLeft="@+id/key4_id"
        android:layout_alignStart="@+id/key4_id" />

    <Button

        android:id="@+id/key6_id"

        android:layout_width="70dp"

        android:layout_height="60dp"

        android:text="6"

        android:onClick="key6Clicked"

        android:layout_below="@+id/key3_id"
        android:layout_toRightOf="@+id/key5_id"
        android:layout_toEndOf="@+id/key5_id" />

    <Button

        android:id="@+id/Buttonclear_id"

        android:layout_height="60dp"

        android:text="clear"

        android:onClick="btnclearClicked"

        android:layout_width="60dp"
        android:background="@android:color/holo_orange_dark"
        android:textColor="@android:color/white"
        android:layout_below="@+id/result_id"
        android:layout_toRightOf="@+id/key6_id"
        android:layout_toEndOf="@+id/key6_id" />

    <Button

        android:id="@+id/Buttonminus_id"

        android:layout_height="60dp"

        android:text="-"

        android:onClick="btnminusClicked"


        android:layout_width="60dp"
        android:layout_below="@+id/Buttonplus_id"
        android:layout_toRightOf="@+id/key9_id"
        android:layout_toEndOf="@+id/key9_id" />


    <EditText

        android:id="@+id/result_id"

        android:layout_width="fill_parent"

        android:layout_height="120dp"

        android:singleLine="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:src='@drawable/owl'
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_above="@+id/Buttonclear_id"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"/>

    <Button


        android:id="@+id/Btnequal_id"

        android:text="="

        android:onClick="btnequalClicked"


        android:layout_height="60dp"
        android:layout_width="130dp"
        android:elevation="0dp"
        android:background="@android:color/holo_red_dark"
        android:textColor="@android:color/white"
        android:layout_below="@+id/key9_id"
        android:layout_toRightOf="@+id/Btndivide_id"
        android:layout_toEndOf="@+id/Btndivide_id" />

    <Button

        android:id="@+id/Btn1_id"

        android:layout_width="70dp"

        android:layout_height="60dp"

        android:text="1"

        android:onClick="key1Clicked"

        android:layout_marginLeft="60dp"
        android:layout_marginStart="60dp"
        android:layout_below="@+id/result_id"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


</RelativeLayout>